{
    "title": {
        "text": "Alder (2024)",
        "style": {
            "fontFamily": "Averta",
            "color": "#000000",
            "fontSize": "20px",
            "fontWeight": "bold",
            "fontStyle": "normal"
        },
        "align": "left"
    },
    "subtitle": {
        "text": "",
        "style": {
            "fontFamily": "Averta",
            "color": "#3250c8",
            "fontSize": "17px",
            "fontWeight": "normal",
            "fontStyle": "normal"
        },
        "align": "left"
    },
    "exporting": {},
    "chart": {
        "polar": false,
        "type": "column"
    },
    "xAxis": {
        "index": 0,
        "isX": true,
        "type": "category",
        "labels": {
            "style": {
                "fontSize": "14px"
            }
        },
        "title": {
            "style": {
                "fontFamily": "Default",
                "color": "#666666",
                "fontSize": "20px",
                "fontWeight": "normal",
                "fontStyle": "normal"
            }
        }
    },
    "yAxis": {
        "index": 0,
        "allowDecimals": false,
        "max": null,
        "title": {
            "text": "Prosent",
            "style": {
                "fontFamily": "Default",
                "color": "#666666",
                "fontSize": "14px",
                "fontWeight": "normal",
                "fontStyle": "normal"
            }
        },
        "labels": {
            "style": {
                "fontSize": "15px"
            }
        }
    },
    "plotOptions": {
        "column": {
            "stacking": "normal",
            "grouping": false,
            "dataLabels": {
                "enabled": true,
                "format": "{y}%",
                "style": {
                    "fontSize": "12px",
                    "fontFamily": "Averta"
                }
            }
        },
        "series": {
            "events": {
                "afterLoad": function() {
                    const series = this.chart.series;
                    const hasStemmmer = series.some(s => s.name.includes('Stemmer'));
                    
                    if (hasStemmmer) {
                        series.forEach(s => {
                            if (s.name.includes('svært dårlig') || s.name.includes('ganske dårlig')) {
                                s.update({stack: 'negative'});
                            } else if (s.name.includes('svært godt') || s.name.includes('ganske godt')) {
                                s.update({stack: 'positive'});
                            }
                        });
                    } else {
                        series.forEach(s => {
                            s.update({stack: undefined, grouping: true});
                        });
                    }
                }
            }
        }
    },
    "series": [
        {
            "turboThreshold": 0,
            "type": "column",
            "marker": {
                "symbol": "circle"
            },
            "dashStyle": "Solid"
        }
    ],
    "data": {
        "seriesMapping": [
            {
                "x": 0,
                "y": 1
            }
        ],
        "enablePolling": true,
        "dataRefreshRate": 60
    },
    "pane": {
        "background": []
    },
    "responsive": {
        "rules": []
    },
    "colors": ["#5573e6", "#fa6e3c", "#fad250", "#00785f", "#808080"],
    "credits": {
        "href": "",
        "text": "N=4125, CICEROs klimaundersøkelse"
    },
    "legend": {
        "itemStyle": {
            "fontFamily": "Averta",
            "color": "#333333",
            "fontSize": "14px",
            "fontWeight": "bold",
            "fontStyle": "normal",
            "cursor": "pointer"
        }
    }
}
