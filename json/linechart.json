{
    "chart": {
        "type": "spline"
    },
    "title": {
        "text": "Utvikling over tid",  
        "style": {
            "fontFamily": "Poppins",
            "color": "#000000",
            "fontSize": "20px",
            "fontWeight": "bold"
        },
        "align": "left"
    },
    "xAxis": {
        "type": "category",
        "labels": {
            "style": {
                "fontSize": "14px",
                "fontFamily": "Poppins",
                "color": "#666666"
            }
        }
    },
    "yAxis": {
        "allowDecimals": false,
        "max": null,
        "title": {
            "text": "Prosent",
            "style": {
                "fontFamily": "Poppins",
                "color": "#666666",
                "fontSize": "14px",
                "fontWeight": "normal"
            }
        },
        "labels": {
            "style": {
                "fontSize": "14px",
                "fontFamily": "Poppins"
            }
        }
    },
    "data": {
        "csvURL": "",  
        "enablePolling": true,
        "dataRefreshRate": 60
    },
    "plotOptions": {
        "spline": {
            "marker": {
                "enabled": true,
                "symbol": "circle"
            },
            "lineWidth": 3  
        }
    },
    "series": [
        {
            "color": "#5573e6" 
        },
        {
            "color": "#fa6e3c"  
        },
        {
            "color": "#fad250"  
        },
        {
            "color": "#00785f"  
        },
        {
            "color": "#7896ff"  
        },
        {
            "color": "#9b59b6"  
        }
    ],
    "legend": {
        "itemStyle": {
            "fontFamily": "Poppins",
            "color": "#333333",
            "fontSize": "14px",
            "fontWeight": "normal"
        }
    },
    "credits": {
        "href": "",
        "text": ""  
    },
    "exporting": {
        "buttons": {
            "contextButton": {
                "menuItems": [
                    "downloadPNG",
                    "downloadJPEG",
                    "downloadPDF",
                    "separator",
                    {
                        "text": "Download CSV",
                        "onclick": function () {
                            const csv = this.getCSV(); // Get the CSV string
                            const blob = new Blob([csv], { type: "text/csv;charset=utf-8;" }); // Create a Blob
                            const url = URL.createObjectURL(blob); // Create a downloadable URL
                            const a = document.createElement("a");
                            a.href = url;
                            a.download = "chart-data.csv"; // Name of the downloaded file
                            a.style.display = "none";
                            document.body.appendChild(a);
                            a.click();
                            document.body.removeChild(a); // Clean up
                        }
                    }
                ]
            }
        }
    }
}
